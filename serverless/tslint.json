{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "trailing-comma": [ false ],
        "no-console": false,
        "max-line-length": [ false ],
        "object-literal-sort-keys": false,
        "no-var-requires": false,
        "indent": [
            true,
            "spaces",
            4
        ]
    },
    "rulesDirectory": []
}